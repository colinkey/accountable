# Front end files
/accountable-frontend/.DS_Store
/accountable-frontend/node_modules
/accountable-frontend/dist

/accountable-frontend/# local env files
/accountable-frontend/.env.local
/accountable-frontend/.env.*.local

/accountable-frontend/# Log files
/accountable-frontend/npm-debug.log*
/accountable-frontend/yarn-debug.log*
/accountable-frontend/yarn-error.log*

/accountable-frontend/# Editor directories and files
/accountable-frontend/.idea
/accountable-frontend/.vscode
/accountable-frontend/*.suo
/accountable-frontend/*.ntvs*
/accountable-frontend/*.njsproj
/accountable-frontend/*.sln
/accountable-frontend/*.sw?

# Back end files
# See https://help.github.com/articles/ignoring-files for more about ignoring files.
#
# If you find yourself ignoring temporary files generated by your text editor
# or operating system, you probably want to add a global ignore instead:
#   git config --global core.excludesfile '~/.gitignore_global'

# Ignore bundler config.
/accountable-backend/.bundle

# Ignore the default SQLite database.
/accountable-backend/db/*.sqlite3
/accountable-backend/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/accountable-backend/log/*
/accountable-backend/tmp/*
/accountable-backend/!/log/.keep
/accountable-backend/!/tmp/.keep

# Ignore uploaded files in development
/accountable-backend/storage/*
/accountable-backend/!/storage/.keep

/accountable-backend/.byebug_history

# Ignore master key for decrypting credentials and more.
/accountable-backend/config/master.key
